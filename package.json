{
  "name": "budget-app-2",
  "sideEffects": false,
  "scripts": {
    "postinstall": "npm run generate-clients && patch-package",
    "build": "remix build",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development npm run wrangler",
    "dev": "npm-run-all build --parallel \"dev:*\"",
    "start": "cross-env NODE_ENV=production npm run wrangler",
    "typecheck": "tsc",
    "wrangler": "wrangler pages dev ./public --local --persist",
    "generate": "drizzle-kit generate:sqlite --schema=./app/lib/schema.ts",
    "up": "drizzle-kit up:sqlite",
    "generate-clients": "openapi --input https://ob.gocardless.com/api/v2/swagger.json --output ./generated-sources/gocardless"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.0.4",
    "@remix-run/cloudflare": "^2.6.0",
    "@remix-run/cloudflare-pages": "^2.6.0",
    "@remix-run/react": "^2.6.0",
    "class-variance-authority": "^0.5.2",
    "clsx": "^2.1.0",
    "cross-env": "^7.0.3",
    "drizzle-orm": "^0.29.3",
    "isbot": "^5.1.0",
    "lucide-react": "^0.333.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.6.0",
    "remix-auth-oauth2": "^1.11.1",
    "remix-utils": "^7.5.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240208.0",
    "@remix-run/dev": "^2.6.0",
    "@remix-run/eslint-config": "^2.6.0",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@types/react-grid-layout": "^1.3.5",
    "better-sqlite3": "^9.4.1",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.56.0",
    "npm-run-all": "^4.1.5",
    "openapi-typescript-codegen": "^0.27.0",
    "patch-package": "^8.0.0",
    "remix-flat-routes": "^0.6.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "wrangler": "^3.28.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
